    "start-backend": "./AccessiScan-Website",
    "local-start": "concurrently \"next start\" \"./AccessiScan-Website\""
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.4",
    "@mui/x-charts": "^7.18.0",
    "concurrently": "^9.0.1",
    "next": "^15.0.2-canary.3",
    "react": "^19.0.0-rc-1631855f-20241023",
    "react-dom": "^19.0.0-rc-1631855f-20241023"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0-rc.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
